{
	"container": "Main",
	"template": "main",
	"errorPage": {
		"template": "error",
		"container": "Error"
	},
	"nested": {
		"admin": {
			"template": "main",
			"container": "Admin.Main",
			"permission": "admin.panel",
			"nested": {
				"users": {
					"template": "users",
					"container": "Admin.Users",
					"permission": "admin.users",
					"events": {
						"addUser": {
							"classname": "Register"
						},
						"editUser": {
							"classname": "EditUser"
						},
						"changeUsersByAdmin": {
							"classname": "Admin.DeleteUsers"
						}
					}
				},
				"settings": {
					"template": "settings",
					"container": "Admin.Settings",
					"permission": "admin.settings",
					"events": {
						"changeSiteSettings": {
							"classname": "Admin.ChangeSettings"
						}
					}
				},
				"groups": {
					"template": "groups",
					"container": "Admin.Groups",
					"permission": "admin.groups",
					"events": {
						"createGroup": {
							"classname": "Admin.AddGroup"
						},
						"editGroup": {
							"classname": "Admin.EditGroup"
						},
						"changeGroup": {
							"classname": "Admin.DeleteGroup"
						},
						"add_permission": {
							"classname": "Admin.AddPerm"
						},
						"removePerm": {
							"classname": "Admin.RemovePerm"
						}
					}
				},
				"shops": {
					"template": "shops",
					"container": "Admin.Shops",
					"permission": "admin.shops",
					"events": {
						"addShop": {
							"classname": "Shops.AddShop"
						},
						"changeShop": {
							"classname": "Shops.DeleteShops"
						},
						"editShop": {
							"classname": "Shops.EditShop"
						},
						"addGood": {
							"classname": "Shops.AddGood"
						},
						"changeGoods": {
							"classname": "Shops.DeleteGoods"
						},
						"editGood": {
							"classname": "Shops.EditGood"
						},
						"changeComments": {
							"classname": "Shops.DeleteComments"
						}
					}
				},
				"category": {
					"template": "category",
					"container": "Admin.Category",
					"permission": "admin.category",
					"events": {
						"editCategory": {
							"classname": "Shops.EditCategory"
						},
						"changeCategories": {
							"classname": "Shops.DeleteCategories"
						},
						"addCategory": {
							"classname": "Shops.AddCategory"
						}
					}
				},
				"database": {
					"template": "database",
					"container": "Admin.Database",
					"permission": "admin.database"
				},
				"login": {
					"template": "login",
					"events": {
						"login": {
							"classname": "Login"
						},
						"register": {
							"classname": "Register"
						}
					}
				}
			}
		},
		"sourcecode": {
			"container": "Sourcecode",
			"template": "sourcecode.main",
			"eNested": {
				"asd": {
					"container": "Sourcecode",
					"template": "sourcecode.main"
				}
			}
		},
		"sitemap": {
			"container": "Sitemap",
			"template": "sitemap"
		}
	}
}
